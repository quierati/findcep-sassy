              $(".swControls").remove();
              $("#busca-table").empty();
              console.log('botao search-endereco');
              var uf =  $("#busca-estado").val().toUpperCase();
              var cidade = $("#busca-cidade").val().trim().replace(/\\s/g,'+');
              var rua = $("#busca-logradouro").val().trim().replace(/\\s/g,'+');
              var txt = "";
              var schema = (("https:" == document.location.protocol) ? "https://" : "http://");
              $.ajax({
              url: schema + "api.findcep.com/v1/endereco/" + uf +"/"+ cidade +"/"+ rua,
              method: "GET",
              dataType: "json",
              })
              .done(function (response) {
              if (response.hits.total > 0) {
              for (i=0; i < response.hits.hits.length; i++) {
              txt += "<li id=\\"result-table\">" +
              "<span><b>Logradouro:</b> "+response.hits.hits[i]._source.logradouro +" "+response.hits.hits[i]._source.complemento+"</span>" +
              "<span><b>Bairro:</b> "+response.hits.hits[i]._source.bairro+"</span>" +
              "<span><b>Cidade/UF:</b> "+response.hits.hits[i]._source.cidade+"/"+response.hits.hits[i]._source.uf+"</span>" +
              "<span><b>CEP:</b> "+response.hits.hits[i]._source.cep+"</span>" +
              "</li>";
              }
              if(txt != ""){
              $("#main").removeClass("hidden");
              $("#busca-table").append(txt).removeClass("hidden");
              $('.holder').sweetPages({perPage:10});
              var controls = $('.swControls').detach();
              controls.appendTo('#main');
              }
              } else {
              console.log("Endereço não foi localizado. Não use abreviações");
              }
              })
              .fail(function (response) {
              console.log(response);
              })
              .always(function (response) {
              ga('send', 'event', 'FindCEP API Endereço', rua + '|' + cidade + '|' + uf, response.hits.total);
              console.log("Busca Endereço: " +response.hits.total+ " endereços");
              });
              });
