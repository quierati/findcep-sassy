            script(type='text/javascript').
              $(document).ready(function () {
              function limpa_formulário_cep() {
              $("#logradouro").val("");
              $("#bairro").val("");
              $("#cidade").val("");
              $("#estado").val("");
              }
              $("#search-cep").click(function () {
              var cep = $("#cep").val().replace(/\\D/g, '');
              var validacep = /^[0-9]{8}$/;
              if (cep != "") {
              if (!validacep.test(cep)) {
              $('#cep-incorrect').addClass("show");
              } else {
              var schema = (("https:" == document.location.protocol) ? "https://" : "http://");
              var url = schema + 'api.findcep.com/v1/cep/' + cep + '.json';
              var request_time = 0;
              var start_time = 0;
              $.ajax({
              url: url,
              method: "GET",
              dataType: 'json',
              beforeSend: function (request, settings) {
              start_time = new Date().getTime();
              },
              })
              .done(function (response) {
              request_time = new Date().getTime() - start_time;
              console.log(request_time);
              $('#cep-incorrect').stop(true, true).fadeOut();
              $('.form-group').removeClass("hide");
              $('.col-sm-3').addClass("success");
              $('.col-sm-3').removeClass("failed");
              $("#logradouro").val(response.logradouro);
              $("#bairro").val(response.bairro);
              $("#cidade").val(response.cidade);
              $("#estado").val(response.uf);
              })
              .fail(function (response) {
              request_time = new Date().getTime() - start_time;
              $('.cep-result').addClass("hide");
              $('#cep-incorrect').stop(true, true).fadeOut();
              $('#cep-incorrect').fadeIn(100).fadeOut(10000);
              $('.col-sm-3').addClass("failed");
              $('.col-sm-3').removeClass("success");
              limpa_formulário_cep();
              })
              .always(function (response) {
              ga('send', 'event', 'FindCEP API', cep, request_time+"ms");
              });
              }
              }
              });
              });
